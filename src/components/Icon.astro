---
import { iconPaths } from '../utils/icons';

interface Props {
  iconName: keyof typeof iconPaths;
}

const { iconName } = Astro.props;
const iconKey = iconPaths[iconName];
---

<svg
  xmlns='http://www.w3.org/2000/svg'
  width='32'
  height='32'
  fill={iconKey.color}
  viewBox='0 0 256 256'
>
  <g set:html={iconKey.path} />
</svg>
